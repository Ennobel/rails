<h1>Listing Articles</h1>
<%= link_to 'New article', new_article_path %>

<table>
<tr>
	<th>Title</th>
	<th>Text</th>
	<th></th>
</tr>

<% @article.each do |article| %>
<tr>
	<th><%= article.title %></th>
	<th><%= article.text %></th>
	<th><%= link_to 'show',article_path(article) %></th>
	<th> <%= link_to 'edit', edit_article_path(article) %> </th>
	<th><%= link_to 'delete', article_path(article), method: :delete,
	data: {confirm: 'Are you sure?' } %> </th>
</tr>
<% end %>
</table>

<%= link_to 'HomePage', controller: 'welcome' %>